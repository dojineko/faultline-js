{
  "compilerOptions": {
    "lib": ["es2015", "dom"],
    "target": "es5",
    "module": "none",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true
  }
}
